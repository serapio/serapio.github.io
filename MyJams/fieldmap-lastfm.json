{
  "fields":[
    { "tags": "return d('.tags').text()"},
    { "wiki-text": "return d('.wiki-text').text()" },
    { "meta-desc": "return d('meta[name=description]').attr('content')"},
	{ "description": "return d('.wiki-text').text() + ' [' + d('.tags').text() + '] ' + d('.lyrics-snippet').text() + ' -- ' + d('meta[name=description]').attr('content')"},
	{ "text": ""},
  	{ "song-name": "return d('.track-overview h1 > span[itemprop=name]').text()" },
    { "song-artist": "return d('span[itemprop=Artist] > meta[itemprop=name]').attr('content')" },
    { "song-album": "return d('span[itemprop=inAlbum] > meta[itemprop=name]').attr('content')" },
    { "artist-name": "return d('.artist-overview h1[itemprop=name]').text()" },
    { "album-name": "return d('.album-overview h1[itemprop=name]').text()"},
    { "album-artist": "return d('album-overview .top-crumb').text()"},
    { "lyrics": "return d('.lyrics-snippet').text()"},
    { "song-count": "count = d('meta[itemprop=interactionCount]').attr('content').split(':')[1]\nreturn count if int(count) > 100000 else ''"},
    { "customrank1": "return len(d('meta[itemprop=interactionCount]').attr('content').split(':')[1])"}
  	]
}